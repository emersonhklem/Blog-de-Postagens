
<h1>{{categoria.nome}}</h1>
{{#each postagens}}
<div class="card mt-4">
    <div class="card-body">
        <h3>{{titulo}}</h3>
        <p>{{descricao}}</p>
        <a href="/postagem/{{slug}}" class="">Leia mais...</a>
        <hr>

         <small> Data de Publicação: {{dati}}</small>
    </div>
</div>
{{else}}
     <h3> Nenhuma Postagem Registrada!</h3>
{{/each}}

